# RNN runs

## CORN test run

python rnn_corn.py --cuda 1 --seed 0 --epochs 1 --dataset tripadvisor --batchsize 16 --numworkers 1 --outpath runs/tripadvisor/rnn_corn-imp0-seed0-epochs1-batchsize16_best-rmse

python rnn_corn.py --cuda -1 --seed 0 --epochs 1 --dataset coursera --batchsize 16 --numworkers 1 --outpath runs/coursera/rnn_corn-imp0-seed0-epochs1-batchsize16_best-rmse

python rnn_corn.py --cuda 1 --seed 0 --epochs 200 --dataset tripadvisor --batchsize 16 --numworkers 1 --outpath runs/tripadvisor/rnn_corn-imp0-seed0-epochs200-batchsize16_best-rmse

python rnn_corn.py --cuda 1 --seed 0 --epochs 200 --dataset coursera --batchsize 16 --numworkers 1 --outpath runs/coursera/rnn_corn-imp0-seed0-epochs200-batchsize16_best-rmse

## CORAL test run

python rnn_coral.py --cuda -1 --seed 0 --epochs 1 --dataset tripadvisor --outpath runs/tripadvisor/rnn_coral-imp0-seed0-epochs1_best-rmse

python rnn_coral.py --cuda -1 --seed 0 --epochs 1 --dataset coursera --outpath runs/coursera/rnn_coral-imp0-seed0-epochs1_best-rmse

python rnn_coral.py --cuda 2 --seed 0 --epochs 200 --dataset tripadvisor --outpath runs/tripadvisor/rnn_coral-imp0-seed0-epochs200_best-rmse

python rnn_coral.py --cuda 2 --seed 0 --epochs 200 --dataset coursera --outpath runs/coursera/rnn_coral-imp0-seed0-epochs200_best-rmse

## NIU test run

python rnn_niu.py --cuda -1 --seed 0 --epochs 1 --dataset tripadvisor --outpath runs/tripadvisor/rnn_niu-imp0-seed0-epochs1_best-rmse

python rnn_niu.py --cuda -1 --seed 0 --epochs 1 --dataset coursera --outpath runs/coursera/rnn_niu-imp0-seed0-epochs1_best-rmse

python rnn_niu.py --cuda 3 --seed 0 --epochs 200 --dataset tripadvisor --outpath runs/tripadvisor/rnn_niu-imp0-seed0-epochs200_best-rmse

python rnn_niu.py --cuda 3 --seed 0 --epochs 200 --dataset coursera --outpath runs/coursera/rnn_niu-imp0-seed0-epochs200_best-rmse

## XENTR test run

python rnn_xentr.py --cuda -1 --seed 0 --epochs 1 --dataset tripadvisor --outpath runs/tripadvisor/rnn_xentr-imp0-seed0-epochs1_best-rmse

python rnn_xentr.py --cuda -1 --seed 0 --epochs 1 --dataset coursera --outpath runs/coursera/rnn_xentr-imp0-seed0-epochs1_best-rmse

python rnn_xentr.py --cuda 4 --seed 0 --epochs 200 --dataset tripadvisor --outpath runs/tripadvisor/rnn_xentr-imp0-seed0-epochs200_best-rmse

python rnn_xentr.py --cuda 4 --seed 0 --epochs 200 --dataset coursera --outpath runs/coursera/rnn_xentr-imp0-seed0-epochs200_best-rmse



